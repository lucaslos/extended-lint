    "@typescript-eslint/parser": "^7.7.0",
    "@typescript-eslint/rule-tester": "^7.7.0",
    "@vitest/ui": "^1.5.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "tsm": "^2.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^7.7.0",
    "escape-string-regexp": "^5.0.0",
    "espree": "^10.0.1",
    "esutils": "^2.0.3",
    "magic-string": "^0.30.10"
