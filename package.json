      "require": "./dist/extended-lint.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@ls-stack/utils": "^1.8.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.5.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@typescript-eslint/rule-tester": "^8.2.0",
    "@vitest/ui": "^2.0.5",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.1",
    "tsm": "^2.3.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.2.0",
    "escape-string-regexp": "^5.0.0",
    "espree": "^10.1.0",
    "esutils": "^2.0.3"
  },
  "peerDependencies": {
    "eslint": "^9.9.1"
