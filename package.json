  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build": "vitest run && tsm esbuild.ts",
    "npm-publish": "scripts/check-if-is-sync.sh && pnpm build && npm publish --access=public"
  },
  "files": [
    "dist"
  ],
